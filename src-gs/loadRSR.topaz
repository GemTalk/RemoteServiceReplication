
! Topaz format...

run
	| rsrRoot loadSpecFile loadSpec resolvedProject |
	Rowan projectTools trace startTracing.
	rsrRoot := '$RSR_ROOT' asFileReference.
	loadSpecFile := rsrRoot / 'rowan'/ 'specs' / 'RemoteServiceReplication', 'ston'.
	loadSpec := (Rowan projectTools load loadProjectSpecFromFile: loadSpecFile)
		projectsHome: rsrRoot parent;
		yourself.
	resolvedProject := loadSpec resolve.

	resolvedProject load.
%
